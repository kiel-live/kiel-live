type Trip {
  id: ID!
  provider: String!
  vehicleId: String!
  direction: String!
  arrivals: [TripArrival!]!
  path: [Location!]!
}

type TripArrival {
  id: ID!
  name: String!
  state: String!
  planned: String!
}

input TripInput {
  id: ID!
  provider: String!
  vehicleId: String!
  direction: String!
  arrivals: [TripArrivalInput!]!
  path: [LocationInput!]!
}

input TripArrivalInput {
  id: ID!
  name: String!
  state: String!
  planned: String!
}