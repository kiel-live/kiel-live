# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Query {
  stops: [Stop!]!
  vehicles: [Vehicle!]!
  trips: [Trip!]!
  routes: [Route!]!

  stop(id: ID!): Stop!
  vehicle(id: ID!): Vehicle!
  trip(id: ID!): Trip!
  route(id: ID!): Route!
}

# map: vehicle (location, type), stops (location, type)
# trip-popup: trip (name, type, stops)
# bus-stop-popup: stop (name, type, arrivals, routes)
