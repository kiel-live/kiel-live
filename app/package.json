{
  "name": "@kiel-live/app",
  "author": "kiel-live.github.io Team",
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "v24.12.0",
    "pnpm": "10.27.0"
  },
  "scripts": {
    "start": "VITE_BUILD_DATE=$(date '+%d.%m.%Y %H:%M:%S') vite",
    "build": "VITE_BUILD_DATE=$(date '+%d.%m.%Y %H:%M:%S') vite build",
    "preview": "vite preview",
    "lint": "eslint --max-warnings 0 .",
    "formatcheck": "prettier -c .",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:update": "docker run --rm -it -v $(pwd):/app --network=host mcr.microsoft.com/playwright:v1.57.0 bash -c \"npm i -g corepack && cd /app && pnpm run test:e2e -u\" && sudo chown -R $USER:$USER tests test-results dist",
    "translate": "pnpx @geprog/translator translate ./src/locales"
  },
  "dependencies": {
    "@vueuse/core": "14.1.0",
    "canvas-confetti": "1.9.4",
    "fuse.js": "7.1.0",
    "maplibre-gl": "5.1.1",
    "micromark": "4.0.2",
    "nats.ws": "1.30.3",
    "vue": "3.5.24",
    "vue-i18n": "11.1.12",
    "vue-router": "4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "6.7.3",
    "@eslint/js": "9.39.2",
    "@iconify/json": "2.2.421",
    "@intlify/eslint-plugin-vue-i18n": "4.1.0",
    "@intlify/unplugin-vue-i18n": "11.0.3",
    "@playwright/test": "1.57.0",
    "@tailwindcss/vite": "4.1.18",
    "@types/canvas-confetti": "1.9.0",
    "@types/geojson": "7946.0.16",
    "@types/node": "24.10.4",
    "@vitejs/plugin-vue": "6.0.3",
    "eslint": "9.39.2",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-vue-scoped-css": "2.12.0",
    "prettier": "3.7.4",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "unplugin-icons": "22.5.0",
    "unplugin-vue-components": "30.0.0",
    "vite": "7.3.0",
    "vite-plugin-pwa": "1.2.0",
    "vitest": "4.0.16",
    "vue-tsc": "3.2.1",
    "workbox-build": "7.4.0",
    "workbox-window": "7.4.0"
  },
  "packageManager": "pnpm@10.27.0+sha512.72d699da16b1179c14ba9e64dc71c9a40988cbdc65c264cb0e489db7de917f20dcf4d64d8723625f2969ba52d4b7e2a1170682d9ac2a5dcaeaab732b7e16f04a"
