{
  "name": "@kiel-live/app",
  "author": "kiel-live.github.io Team",
  "version": "0.0.0",
  "engines": {
    "node": "14 || 16 || 18",
    "pnpm": "8"
  },
  "scripts": {
    "start": "VITE_BUILD_DATE=$(date '+%d.%m.%Y %H:%M:%S') vite",
    "build": "VITE_BUILD_DATE=$(date '+%d.%m.%Y %H:%M:%S') vite build",
    "serve": "vite preview",
    "lint": "eslint --max-warnings 0 --ext .js,.ts,.vue,.json .",
    "formatcheck": "prettier -c .",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@vueuse/core": "9.13.0",
    "canvas-confetti": "1.6.0",
    "fuse.js": "6.6.2",
    "maplibre-gl": "3.0.0-pre.3",
    "nats.ws": "1.13.1",
    "vue": "3.2.47",
    "vue-i18n": "9.2.2",
    "vue-router": "4.1.6"
  },
  "devDependencies": {
    "@iconify/json": "2.2.25",
    "@intlify/unplugin-vue-i18n": "0.8.2",
    "@types/canvas-confetti": "1.6.0",
    "@types/geojson": "7946.0.10",
    "@types/node": "18.14.0",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "@vitejs/plugin-vue": "4.0.0",
    "eslint": "8.34.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-vue": "9.9.0",
    "eslint-plugin-vue-scoped-css": "2.4.0",
    "prettier": "2.8.4",
    "typescript": "4.9.5",
    "unplugin-icons": "0.15.3",
    "unplugin-vue-components": "0.24.0",
    "vite": "4.1.4",
    "vite-plugin-pwa": "0.14.4",
    "vite-plugin-windicss": "1.8.10",
    "vitest": "0.28.5",
    "vue-eslint-parser": "9.1.0",
    "vue-tsc": "1.1.5",
    "windicss": "3.5.6",
    "workbox-window": "6.5.4"
