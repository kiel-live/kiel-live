{
  "name": "@kiel-live/app",
  "author": "kiel-live.github.io Team",
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "18 || 20 || 22",
    "pnpm": "9"
  },
  "scripts": {
    "start": "VITE_BUILD_DATE=$(date '+%d.%m.%Y %H:%M:%S') vite",
    "build": "VITE_BUILD_DATE=$(date '+%d.%m.%Y %H:%M:%S') vite build",
    "serve": "vite preview",
    "lint": "eslint --max-warnings 0 .",
    "formatcheck": "prettier -c .",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "translate": "pnpx @geprog/translator translate ./src/locales"
  },
  "dependencies": {
    "@vueuse/core": "11.0.3",
    "canvas-confetti": "1.9.3",
    "fuse.js": "7.0.0",
    "maplibre-gl": "4.7.0",
    "micromark": "4.0.0",
    "nats.ws": "1.29.2",
    "vue": "3.5.3",
    "vue-i18n": "9.14.0",
    "vue-router": "4.4.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.22.3",
    "@eslint/js": "^9.7.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@iconify/json": "2.2.245",
    "@intlify/eslint-plugin-vue-i18n": "^3.0.0",
    "@intlify/unplugin-vue-i18n": "4.0.0",
    "@types/canvas-confetti": "1.6.4",
    "@types/eslint__js": "^8.42.3",
    "@types/geojson": "7946.0.14",
    "@types/node": "20.12.8",
    "@typescript-eslint/eslint-plugin": "6.19.1",
    "@typescript-eslint/parser": "6.19.1",
    "@vitejs/plugin-vue": "5.1.3",
    "eslint": "^9.10.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "^6.4.0",
    "eslint-plugin-simple-import-sort": "12.0.0",
    "eslint-plugin-vue": "9.21.1",
    "eslint-plugin-vue-scoped-css": "^2.8.1",
    "prettier": "3.3.3",
    "typescript": "5.4.5",
    "typescript-eslint": "^7.16.1",
    "unplugin-icons": "0.19.3",
    "unplugin-vue-components": "0.27.4",
    "vite": "5.4.3",
    "vite-plugin-pwa": "0.20.5",
    "vite-plugin-windicss": "1.9.3",
    "vitest": "2.0.5",
    "vue-tsc": "2.1.6",
    "windicss": "3.5.6",
    "workbox-window": "7.1.0"
  }
}
