  "scripts": {
    "start": "VITE_BUILD_DATE=$(date '+%d.%m.%Y %H:%M:%S') vite",
    "build": "VITE_BUILD_DATE=$(date '+%d.%m.%Y %H:%M:%S') vite build",
    "preview": "vite preview",
    "lint": "eslint --max-warnings 0 .",
    "formatcheck": "prettier -c .",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:update": "docker run --rm -it -v $(pwd):/app --network=host mcr.microsoft.com/playwright:v1.58.0 bash -c \"npm i -g corepack && cd /app && pnpm run test:e2e -u\" && sudo chown -R $USER:$USER tests test-results dist",
    "translate": "pnpx @geprog/translator translate ./src/locales"
  },
  "dependencies": {
    "@vueuse/core": "14.1.0",
    "canvas-confetti": "1.9.4",
