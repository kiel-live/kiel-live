{
  "name": "@kiel-live/app",
  "author": "kiel-live.github.io Team",
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "18 || 20 || 22",
    "pnpm": "9"
  },
  "scripts": {
    "start": "VITE_BUILD_DATE=$(date '+%d.%m.%Y %H:%M:%S') vite",
    "build": "VITE_BUILD_DATE=$(date '+%d.%m.%Y %H:%M:%S') vite build",
    "serve": "vite preview",
    "lint": "eslint --max-warnings 0 .",
    "formatcheck": "prettier -c .",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "translate": "pnpx @geprog/translator translate ./src/locales"
  },
  "dependencies": {
    "@vueuse/core": "12.2.0",
    "canvas-confetti": "1.9.3",
    "fuse.js": "7.0.0",
    "maplibre-gl": "4.7.1",
    "micromark": "4.0.1",
    "nats.ws": "1.29.2",
    "vue": "3.5.13",
    "vue-i18n": "10.0.5",
    "vue-router": "4.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "3.14.0",
    "@eslint/js": "9.17.0",
    "@iconify/json": "2.2.288",
    "@intlify/eslint-plugin-vue-i18n": "3.2.0",
    "@intlify/unplugin-vue-i18n": "6.0.3",
    "@types/canvas-confetti": "1.9.0",
    "@types/eslint__js": "8.42.3",
    "@types/geojson": "7946.0.15",
    "@types/node": "22.10.2",
    "@vitejs/plugin-vue": "5.2.1",
    "eslint": "9.17.0",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-vue-scoped-css": "2.9.0",
    "prettier": "3.4.2",
    "typescript": "5.7.2",
    "unplugin-icons": "0.22.0",
    "unplugin-vue-components": "0.28.0",
    "vite": "6.0.5",
    "vite-plugin-pwa": "0.21.1",
    "vite-plugin-windicss": "1.9.4",
    "vitest": "2.1.8",
    "vue-tsc": "2.1.10",
    "windicss": "3.5.6",
    "workbox-window": "7.3.0"
  }
}
